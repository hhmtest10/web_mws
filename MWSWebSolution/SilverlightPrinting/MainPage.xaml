        <TextBox Height="123" HorizontalAlignment="Left" Margin="12,0,0,62" Name="PrinterCommandtextBox1" VerticalAlignment="Bottom" Width="351" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="41,51,0,0" Name="PrinterNametextBox2" VerticalAlignment="Top" Width="120" />
        <sdk:Label Height="28" HorizontalAlignment="Left" Margin="41,19,0,0" Name="PrinterNameLabel" VerticalAlignment="Top" Width="120" Content="PrinterName" />
    </Grid>
